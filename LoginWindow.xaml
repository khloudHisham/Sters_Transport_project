<Window x:Class="StersTransport.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StersTransport"
           xmlns:CustomeControls="clr-namespace:StersTransport.CustomeControls"
        mc:Ignorable="d"
        Title="LoginWindow" Height="700" Width="1000"
        WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded"
        x:Name="window_login"
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
        SizeChanged="Window_SizeChanged"
        StateChanged="window_login_StateChanged"
         PreviewKeyDown="window_login_PreviewKeyDown"
        MinWidth="800" MinHeight="600"
        >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4.6*"></ColumnDefinition>
            <ColumnDefinition Width="6.5*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="2.5*"></RowDefinition>
            <RowDefinition x:Name="row2" Height="20*"></RowDefinition>
            <RowDefinition Height="1.5*"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Background="#E4E4E4" Grid.Row="1" Panel.ZIndex="-1">
            <Path   
              Stroke="Black" StrokeThickness=".1"  >
                <Path.Fill>
                    <RadialGradientBrush
                        GradientOrigin="0.6,0.5"
                        Center="0.6,0.4" 
                        RadiusX=".5"
                        RadiusY=".5"
                        >
                        <RadialGradientBrush.GradientStops>
                            <GradientStop Color="#236BC9" Offset="1" />
                            <GradientStop Color="#E4E4E4"  Offset=".01" />
             
                        </RadialGradientBrush.GradientStops>

                    </RadialGradientBrush>

                     
                </Path.Fill>
                <Path.Data>
                    <EllipseGeometry  x:Name="elips" 
            Center="100,300"
            RadiusX="250" 
            RadiusY="350"
                        />
                </Path.Data>
            </Path>
        </Grid>




        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"></ColumnDefinition>
                <ColumnDefinition Width="1*"></ColumnDefinition>
                <ColumnDefinition Width="2*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Image  Source="pack://application:,,,/Resources/sters.png"></Image>
            <Border 
                Margin="10,20,10,20" Grid.Column="1" CornerRadius="5"
                Background="#236BC9">

            </Border>
            <TextBlock VerticalAlignment="Center" 
                       HorizontalAlignment="Center"
                       Foreground="White"
                       FontSize="18"
                       FontWeight="Bold"
                       Grid.Column="1">EUKnet</TextBlock>
            <TextBlock VerticalAlignment="Center" 
                       HorizontalAlignment="Center"
                       Foreground="Black"
                       FontSize="18"
                       FontWeight="Bold"
                       Grid.Column="2">TRANSPORT COMPANY</TextBlock>
        </Grid>



        <Grid  Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="2*"></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition Height="2*"></RowDefinition>
            </Grid.RowDefinitions>

            <Image Grid.Row="1" Source="pack://application:,,,/Resources/sters.png"></Image>
            <TextBlock VerticalAlignment="Center"
                       HorizontalAlignment="Center"
                       FontSize="18"
                       FontWeight="Bold"
                       Grid.Row="2"> Welcome To Our Wonderful World </TextBlock>
        </Grid >

        <Grid  Background="#E4E4E4" Grid.Row="1" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width=".5*"> </ColumnDefinition>
                <ColumnDefinition Width=".5*"></ColumnDefinition>
                <ColumnDefinition Width="1.5*"></ColumnDefinition>
                <ColumnDefinition Width="1*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition  Height="1*"></RowDefinition>
                <RowDefinition  Height="1*"></RowDefinition>
                <RowDefinition  Height="1*"></RowDefinition>
            </Grid.RowDefinitions>



            <Border 
                Grid.Row="2"
                Margin="5,10,5,10" Grid.Column="1" CornerRadius="5"
                Background="#236BC9">

            </Border>
            <TextBlock    Grid.Row="2" VerticalAlignment="Center" 
                       HorizontalAlignment="Center"
                       Foreground="White"
                       FontSize="18"
                       FontWeight="Bold"
                       Grid.Column="1">EUKnet</TextBlock>
            <TextBlock    Grid.Row="2" VerticalAlignment="Center" 
                       HorizontalAlignment="Center"
                       Foreground="Black"
                       FontSize="18"
                       FontWeight="Bold"
                       Grid.Column="2">TRANSPORT COMPANY</TextBlock>




