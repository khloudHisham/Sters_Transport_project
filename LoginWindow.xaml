                                <ScaleTransform.ScaleY>.5</ScaleTransform.ScaleY>
                            </ScaleTransform>
                        </Path.RenderTransform>
                    </Path>
                </Grid>
                
            </Viewbox>

            <CustomeControls:ComboboxEX FontWeight="Bold"  Grid.Row="4" Grid.Column="2" x:Name="cmb_officeName"
